<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>cs2-web-login</AssemblyName>
    <GenerateAssemblyInfo>True</GenerateAssemblyInfo>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <TargetFramework>netcoreapp8.0</TargetFramework>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>11.0</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup />
  <ItemGroup>
    <PackageReference Include="BCrypt.NET-Next" Version="4.0.3" />
    <PackageReference Include="CounterStrikeSharp.API" Version="1.0.246" />
    <PackageReference Include="MySQLConnector" Version="2.3.7" />
    <PackageReference Include="PusherClient" Version="2.2.1" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="CasesAPI">
      <HintPath>lib/CasesAPI.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
